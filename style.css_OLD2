body {
  margin: 0;
  padding: 0;
  background-color: #F1F0F0;
  overflow-x: hidden;  }

.contact-masthead {
  display: block;
  width: 100%;
  margin: 0px auto; }
  .contact-masthead > .info {
    padding: 5px;
    background-color: #304030;
    text-align: center;
    color: #fff; }
    .contact-masthead > .info > ul {
      display: list-item;
      margin: 0 auto;
      padding: 0; }
      .contact-masthead > .info > ul li {
        display: inline;
        font-size: 14px; }
        .contact-masthead > .info > ul li:before {
          display: inline;
          content: '\2022';
          color: #B9D95A;
          margin-right: 5px; }
        .contact-masthead > .info > ul li:first-child {
          margin-right: 25px; }

.od-navbar {
  /* background-color: #120026; */
  background: rgba(7, 0, 15, 0.7);
  margin-bottom: 0px;
  border-width: 0px;
  margin-top: 0px;
  z-index: 199; }
  @media (min-width: 768px) {
    .od-navbar {
      height: 60px; } }
  @media (min-width: 992px) {
    .od-navbar {
      height: 70px; } }
  @media (min-width: 1200px) {
    .od-navbar {
      height: 100px; } }
  @media (min-width: 1920px) {
    .od-navbar {
      margin-left: auto;
      margin-right: auto; } }
  .od-navbar .navbar-brand {
    color: #fff;
    font-weight: bold;
    font-size: 28px;
    padding: 10px 0px 10px 15px; }
    @media (min-width: 768px) {
      .od-navbar .navbar-brand {
        height: 60px; }
        .od-navbar .navbar-brand img {
          height: 40px !important; } }
    @media (min-width: 992px) {
      .od-navbar .navbar-brand img {
        height: 50px !important; } }
    @media (min-width: 1200px) {
      .od-navbar .navbar-brand {
        padding: 5px 0px 5px 15px; }
        .od-navbar .navbar-brand img {
          height: 80px !important; } }
    .od-navbar .navbar-brand:hover, .od-navbar .navbar-brand:focus {
      color: #cccccc; }
    .od-navbar .navbar-brand img {
      height: 30px; }
  @media (min-width: 768px) {
    .od-navbar #od-header-menu {
      margin-top: 5px; } }
  @media (min-width: 992px) {
    .od-navbar #od-header-menu {
      margin-top: 9px; } }
  @media (min-width: 1200px) {
    .od-navbar #od-header-menu {
      margin-top: 21px; } }
  .od-navbar #od-header-menu li {
    padding-top: 7px;
    padding-left: 10px;
    padding-right: 10px; }
    @media (min-width: 992px) {
      .od-navbar #od-header-menu li {
        padding-left: 30px;
        padding-right: 30px; } }
    .od-navbar #od-header-menu li a {
      text-transform: uppercase;
      color: #fff;
      padding: 8px;
      font-weight: bold;
      border-radius: 5px; }
      .od-navbar #od-header-menu li a:hover, .od-navbar #od-header-menu li a.active {
        background-color: #A9C652; }
    .od-navbar #od-header-menu li.current-menu-item a {
      background-color: #A9C652; }
  .od-navbar .navbar-toggle {
    border-color: transparent; }
    .od-navbar .navbar-toggle:hover, .od-navbar .navbar-toggle:focus {
      background-color: #4c584c; }
      .od-navbar .navbar-toggle:hover .icon-bar, .od-navbar .navbar-toggle:focus .icon-bar {
        background-color: #cccccc; }
    .od-navbar .navbar-toggle .icon-bar {
      background-color: #fff; }


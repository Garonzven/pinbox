#slider_tablet,
#slider_mobile,
#slider_desktop {
  position: relative;
  margin: 0 auto;
  top: 0px;
  left: 0px;
  overflow: hidden;
  visibility: hidden; }
  #slider_tablet .carousel .carousel-title,
  #slider_mobile .carousel .carousel-title,
  #slider_desktop .carousel .carousel-title {
    position: absolute;
    background-color: rgba(255, 255, 255, 0.65);
    padding: 8px;
    opacity: 0;
    top: 0;
    transition: opacity .5s linear; }
    #slider_tablet .carousel .carousel-title:hover,
    #slider_mobile .carousel .carousel-title:hover,
    #slider_desktop .carousel .carousel-title:hover {
      opacity: 1; }
  #slider_tablet .carousel .btn-carousel,
  #slider_mobile .carousel .btn-carousel,
  #slider_desktop .carousel .btn-carousel {
    position: absolute;
    right: 8px;
    bottom: 8px;
    background-color: #86a726;
    font-size: 10px;
    padding: 2px 4px;
    border: 1px solid #86a726; }
    #slider_tablet .carousel .btn-carousel:hover,
    #slider_mobile .carousel .btn-carousel:hover,
    #slider_desktop .carousel .btn-carousel:hover {
